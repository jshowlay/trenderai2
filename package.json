{
  "name": "trenderai-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:check": "node scripts/check-db.js",
    "db:health": "curl -s http://localhost:3000/api/health/db | jq .",
    "db:migrate": "node scripts/migrate.js",
    "db:migrate:status": "node scripts/migrate.js status",
    "db:seed": "node scripts/seed.js",
    "db:seed:force": "node scripts/seed.js --force",
    "db:seed:reset": "node scripts/seed.js --reset",
    "db:seed:reset:seed": "node scripts/seed.js --reset --seed",
    "db:seed:summary": "node scripts/seed.js --summary",
    "db:setup": "npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@types/pg": "^8.11.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.344.0",
    "next": "14.2.5",
    "next-themes": "^0.4.4",
    "pg": "^8.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4"
  }
}
